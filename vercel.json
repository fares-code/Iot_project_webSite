{
  "version": 2,
  "builds": [
    {
      "src": "server.js", // Path to your Express server file
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)", // API routes start with /api
      "dest": "/server.js" // Directs to your Express server
    },
    {
      "src": "/(.*)", // Catch-all route for other static files or routes
      "dest": "/server.js"
    }
  ]
}
