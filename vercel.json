{
    "version": 2,
    "builds": [
      {
        "src": "server.js", // Your Express server file
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/Routers/(.*)", // Assuming your API routes are prefixed with /api
        "dest": "/server.js" // Directs to your Express server
      },
      {
        "src": "/Routers/(.*)", // Catch-all route for everything else
        "dest": "/server.js"
      }
    ]
  }
  
